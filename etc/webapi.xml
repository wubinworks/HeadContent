<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route url="/V1/wubinworks-injectheaddata" method="GET">
		<service class="Wubinworks\InjectHead\Api\InjectHeadDataManagementInterface" method="getInjectHeadData"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
	<route url="/V1/wubinworks-injectheaddata/:referer" method="GET">
		<service class="Wubinworks\InjectHead\Api\InjectHeadDataManagementInterface" method="getInjectHeadData"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
	<route url="/V1/wubinworks-injectheaddata/:referer/:refererFullActionName" method="GET">
		<service class="Wubinworks\InjectHead\Api\InjectHeadDataManagementInterface" method="getInjectHeadData"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
</routes>
